<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="parent" prefHeight="417.0" prefWidth="537.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladores.PlanDeCuentaController">
   <children>
      <ImageView fx:id="imagenDebajo" fitHeight="367.0" fitWidth="537.0" layoutX="1.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Imagenes/asd.png" />
         </image>
      </ImageView>
      <TableView fx:id="cuentasTableView" prefHeight="352.0" prefWidth="537.0">
        <columns>
          <TableColumn fx:id="Nro_Cuenta" prefWidth="75.0" text="Nro cuenta" />
          <TableColumn fx:id="Cuenta" prefWidth="212.0" text="Cuenta" />
            <TableColumn fx:id="Recibe_Saldo" prefWidth="96.0" text="Recibe Saldo" />
            <TableColumn fx:id="Tipo" prefWidth="78.0" text="Tipo" />
            <TableColumn fx:id="Habilitada" prefWidth="75.0" text="Habilitada" />
        </columns>
      </TableView>
      <Button fx:id="AgregarCuenta" layoutX="14.0" layoutY="368.0" mnemonicParsing="false" onAction="#AgregarCuentaBoton" text="Nueva Cuenta" />
      <Button fx:id="EliminarCuenta" layoutX="111.0" layoutY="368.0" mnemonicParsing="false" onAction="#DeshabilitarCuentaBoton" text="Deshabilitar Cuenta" />
      <Button fx:id="HabilitarCuenta" layoutX="238.0" layoutY="368.0" mnemonicParsing="false" onAction="#HabilitarCuentaBoton" text="Habilitar Cuenta" />
      <Button fx:id="BotonSalir" layoutX="484.0" layoutY="368.0" mnemonicParsing="false" onAction="#Salir" text="Salir" />
   </children>
</AnchorPane>
